{
  "name": "vite-config-for-vue-project",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.ts",
  "scripts": {
    "prepare": "husky install",
    "start": "ts-node --project tsconfig.vite.json --files build/dev-server.ts",
    "build": "vite build --config build/build.ts",
    "preview": "vite preview --host --open --port 8088",
    "lint": "npm run prettier && npm run lint-script && npm run lint-style",
    "prettier": "prettier --ignore-path .eslintignore --write \"./**/*.{vue,js?(x),ts?(x),css,less,scss,sass}\"",
    "lint-script": "eslint --cache --ext .vue,.js,.jsx,.ts,.tsx --ignore-path .eslintignore --fix ./",
    "lint-style": "stylelint --ignore-path .eslintignore --cache --fix src/**/*.{vue,css,less,scss,sass}"
  },
  "author": "jianguo",
  "license": "ISC",
  "devDependencies": {
    "@commitlint/cli": "^16.0.2",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/detect-port": "^1.3.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "detect-port": "^1.3.0",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.7",
    "postcss-html": "^1.3.0",
    "postcss-less": "^6.0.0",
    "prettier": "^2.5.1",
    "style-loader": "^3.3.1",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-prettier": "^2.0.0",
    "ts-node": "^10.5.0",
    "vite-plugin-checker": "^0.4.0"
  },
  "dependencies": {
    "@types/node": "^17.0.8",
    "@vitejs/plugin-vue": "^2.2.0",
    "autoprefixer": "^10.4.2",
    "axios": "^0.24.0",
    "less": "^4.1.2",
    "postcss": "^8.4.5",
    "typescript": "^4.5.4",
    "vite": "^2.8.0",
    "vue": "^3.2.26",
    "vue-router": "^4.0.12",
    "vuex": "^4.0.2",
    "webpack-merge": "^5.8.0"
  }
